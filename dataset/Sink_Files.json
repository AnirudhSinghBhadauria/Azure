{
	"name": "Sink_Files",
	"properties": {
		"linkedServiceName": {
			"referenceName": "azurestorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"filename": {
				"type": "string"
			},
			"filetype": {
				"type": "string"
			}
		},
		"folder": {
			"name": "Copy Files Datasets"
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().filename",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@concat('target/', 'processed files/', dataset().filetype, '/')",
					"type": "Expression"
				},
				"container": "test-storage"
			}
		}
	}
}